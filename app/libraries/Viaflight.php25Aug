	          {
	             break;
	          }
	         $cnt= $cnt+1;
	       }
	             
	       $output = array_slice($array,$cnt); 
	       unset($output['0_attr']);
	       $return=$output;
	      	
	      		
	          $fly['onward']= array_filter($onward);
	         $fly['return']=array_filter($return);
	          
	          // get the destination flghts
		//echo '<pre>';	 	
		//print_r($fly);  exit; 
	      return array_filter($fly);
		
	}
