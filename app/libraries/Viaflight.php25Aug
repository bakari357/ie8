	    if(is_object($obj)) $obj = (array) $obj;
		if(is_array($obj)) {
		$new = array();
	    foreach($obj as $key => $val) {
		$new[$key] = Viaflight::toArray($val);
	}
	}
	  else {
	        $new = $obj;
	       }
	 return $new;
	}


